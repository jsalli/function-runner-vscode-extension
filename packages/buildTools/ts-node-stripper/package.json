{
	"name": "ts-node-stripper",
	"version": "1.0.0",
	"main": "dist/ts-node-stripper.js",
	"types": "dist/ts-node-stripper.d.ts",
	"scripts": {
		"clean": "rimraf ./dist/* tsconfig.tsbuildinfo package.tgz *.log .swc/*",
		"compile": "swc src -d dist",
		"build": "rushx compile && rushx build-types",
		"build-types": "tsc -p tsconfig.build.types.json",
		"test": "echo 'no test'",
		"up-dependencies": "pnpm up",
		"strip-ts-node": "node --no-warnings --experimental-specifier-resolution=node -r ts-node/register src/ts-node-stripper.ts"
	},
	"dependencies": {
		"rimraf": "6.0.1",
		"fs-extra": "11.2.0"
	},
	"devDependencies": {
		"@swc/cli": "0.4.0",
		"@swc/core": "1.7.23",
		"@types/node": "22.5.2",
		"@typescript-eslint/eslint-plugin": "8.4.0",
		"@typescript-eslint/parser": "8.4.0",
		"eslint": "9.10.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-import-resolver-typescript": "3.6.3",
		"eslint-plugin-anti-trojan-source": "1.1.1",
		"eslint-plugin-prettier": "5.2.1",
		"prettier": "3.3.3",
		"typescript": "5.5.4",
		"@types/rimraf": "4.0.5",
		"ts-node": "10.9.2",
		"@types/fs-extra": "11.0.4"
	}
}
